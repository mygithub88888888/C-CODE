       //getchar()语句的用法
//    int main()
//    {
//        char ch = getchar();
//        putchar(ch);
//        printf("%c\n",ch);
//        return 0;
//    }

          int main()
    {
        int ch = 0;
        //getchar()遇到ctrl+z时就停止了
        //EOF  === end of file->-1
        while((ch=getchar()) != EOF)
     {
        putchar(ch);
     }
        return 0;
   }


