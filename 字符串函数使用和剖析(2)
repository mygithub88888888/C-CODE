/*
//实现strcmp函数
int my_strcmp(const char*str1,const char*str2)
{
    assert(str1 && str2);//断言
    //比较
    while(*str1 == *str2)
    {
        if(*str1 == '\0')
        {
            return 0;//相等
        }
        str1++;
        str2++;
    }
    if(*str1 > *str2)
        return 1;
    else
        return -1;

}
int main()
{
     char* p1 = "fafdsf";
     char* p2 = "dasfd";
     int ret = my_strcmp(p1,p2);
     printf("%d\n",ret);
    return 0;
}
