//strncpy---三个参数，多一个拷贝字节个数
int main()
{
    char arr1[10] = "abcdef";
    char arr2[] = "hello bit";
    strncpy(arr1,arr2,4);//拷贝4个字节
    printf("%s\n",arr1);

    return 0;
}
