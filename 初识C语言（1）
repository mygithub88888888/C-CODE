/*
//(二进制)位操作
int main()
{
    //按位与
    //按位或
    //按位异或
    int a = 3;
    int b = 5;
    int c = a&b;//按位与
      int d = a|b;//按位或
        int e = a^b; //按位异或
    //011
    //101
    //001
    printf("%d\n",c);//1
    //011
    //101
    //111
    printf("%d\n",d);//7
    //011
    //101
    //110   计算规律：对应的二进制位相同为0，不同为1
    printf("%d\n",e);//6
    return 0 ;
